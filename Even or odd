#include<stdio.h>
int main()
{
	int a[10],n,i;
	printf("\nenter the no.of elements:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("eneter the elementsa[%d]:",i);
		scanf("%d",&a[i]);
	}
	printf("\nthe even elemets are:\n");
	for(i=0;i<n;i++)
	{
		if(a[i]%2==0)
		{
			printf("%d\n",a[i]);
		}
	}
	printf("\nthe odd element are:\n");
	for(i=0;i<n;i++)
	{
		if(a[i]%2!=0)
		{
			printf("%d\n",a[i]);
		}
	}
	return 0;
}
